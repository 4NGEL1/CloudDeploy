{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "redis": {
    "hostname": "${REDISDATA_SVC_SERVICE_HOST}:${REDISDATA_SVC_SERVICE_PORT}"
  },
  "kafka": {
    "EventHubFQDN": "${KAFKA_SERVICE_SERVICE_HOST}:${KAFKA_SERVICE_SERVICE_PORT}",
    "EventHubConnectionString": ""
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseContext": "server=${MYSQL_DEPLOYMENT_SERVICE_HOST};port=${MYSQL_DEPLOYMENT_SERVICE_PORT};database=${MYSQL_DEPLOYMENT_SERVICE_NAME};uid=${MYSQL_DEPLOYMENT_SERVICE_USER};pwd=${MYSQL_DEPLOYMENT_SERVICE_PASSWORD};Convert Zero Datetime=true;CharSet=utf8;AllowLoadLocalInfile=true;Pooling=true;MinPoolSize=1;MaxPoolSize=300;Connect Timeout=5"
  }
}